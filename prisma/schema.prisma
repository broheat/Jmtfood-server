// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Restaurant {
  id       Int     @id @default(autoincrement())
  area     String
  city     String
  foodType String
  mainFood String?
  name     String
}
